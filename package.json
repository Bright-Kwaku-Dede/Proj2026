{
  "name": "proj2026",
  "version": "1.0.0",
  "description": "A Node.js application with CI/CD setup",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "dev": "nodemon index.js",
    "test": "jest",
    "test:unit": "jest --testPathPattern=tests/unit",
    "test:integration": "jest --testPathPattern=tests/integration",
    "test:e2e": "jest --testPathPattern=tests/e2e",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint .",
    "build": "echo 'No build step needed' && exit 0",
    "docker:build": "docker build -t proj2026 .",
    "docker:run": "docker run -p 3000:3000 proj2026",
    "docker:compose": "docker-compose -f docker-compose.ci.yml up"
  },
  "keywords": [
    "nodejs",
    "express",
    "api",
    "cicd"
  ],
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "eslint": "^10.0.0",
    "@eslint/js": "^10.0.1",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "supertest": "^6.3.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}
